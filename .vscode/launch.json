{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug TrajectoryFollower Node",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/trajectory_follower_nobuakif/lib/trajectory_follower_nobuakif/trajectory_follower_nobuakif_node",
      "args": [
        "--ros-args",
        "-r", "__node:=trajectory_follower_nobuakif_node",
        "-p", "use_external_target_vel:=true",
        "-p", "external_target_vel:=34.9",
        "-p", "yaw_feedback_gain:=0.0",
        "-p", "yaw_rate_feedback_gain:=1.0",
        "-p", "steering_feedback_gain:=0.0",
        "-p", "position_gain_forward:=0.0",
        "-p", "vehicle_model:=1", // 0 = Kinematic, 1 = Dynamic
        "-p", "lookahead_distance:=1.0",
        "-p", "lookahead_gain:=0.5",
        "--remap", "input/kinematics:=/localization/kinematic_state",
        "--remap", "input/trajectory:=/planning/scenario_planning/trajectory",
        "--remap", "output/control_cmd:=/control/command/control_cmd"
      ],
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build trajectory_follower_nobuakif"
    }
  ]
}